# load useful libraries
require(hexbin)
require(inline)
require(multicore)
require(splines)

# set options
curr_repo <- getOption("repos")
cur_repo["CRAN"] <- "http://lib.stat.cmu.edu/CRAN"
options(repos=curr_repo)
options(digits=16) 
options(max.print=100)
options(stringsAsFactors=FALSE)
options(tab.width=2) 
options(continue="\t")

# load useful functions
source("funcs.R")
